{
    "manifest_version": 2,
    "name": "Tab Manager",
    "version": "1.0",
  
    "description": "Gérer les onglets pour les gens mal organisés.",
  
    "icons": {},
  
    "content_scripts": [
        
    ],

    "permissions": [
        "<all_urls>",
        "tabs"
    ],

    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_popup": "popup.html"
    }
}  